  },
  "funding": {
    "url": "https://ko-fi.com/tunnckoCore"
  },
  "scripts": {
    "hela": "node ./src/cli",
    "hela-prod": "./bin/hela-linux",
    "test": "yarn hela && yarn build && yarn node dist",
    "prebundle": "rm -rf dist",
    "bundle": "yarn bundle:cli && yarn bundle:core && yarn bundle:yaro",
    "bundle:cli": "ncc build ./src/cli/index.js -sm -o dist/cli",
    "bundle:core": "ncc build ./src/core/index.js -sm -o dist/core",
    "bundle:yaro": "ncc build ./src/yaro/index.js -sm -o dist/yaro",
    "prebuild": "rm -rf bin",
    "build": "pkg . -o bin/hela -t node10-linux,node10-macos,node10-win",
    "xpostbuild": "ls bin/hela* | xargs gzip",
    "prepublishOnly": "yarn bundle && yarn build"
  },
  "bin": {
    "hela": "./dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/core/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "tag": "next"
  },
  "devDependencies": {
    "@tunnckocore/eslint-config": "^5.2.1",
